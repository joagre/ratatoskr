//type Result<$t1, $t2> = Ok<$t1> || Error<$t2>

fn foo<$t>(x: $t, baz: ($t -> Bool)) -> Result<$t, String> {
   if baz(x) {
       Result;Ok<x> // Constructirs can be prefixed with its type parent
   } else {
       Error<"Bad value">
   }
}

fn bar() {
    switch foo<Int>(42, fn (x) { x == 42 }) { // <Int> can be omitted
        case Ok<?value> =>
            writeln("%value")
        case Error<?reason> =>
            writeln("%reason")
    }
}
