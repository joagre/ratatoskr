export fn main() {
  fn foo(x) {
      x + 1
  },
  ?bar := fn (x) {
      (?a, ?b) := x,
      foo(a) + baz(b)
  },
  ?zippo := fn gazonk(x) {
      gazonk(x) + 1 + bar((x, 42)) + foo(42) + foo(3 + gazonk(11)) // :-)
  },
  (foo(42), bar((4711, 43)), zippo(44), gazonk(106), baz(45))
}

fn baz(x) {
    x + 1
}
