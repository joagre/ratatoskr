; Run: sa -l ./ module_calls 0 10
;
; start(N) ->
;     io:format("~w\n", [fac:fac(N)]),
;     io:format("~w\n", [tfac:fac(N + 2)]).

label 0
   loadrr r3 r1    ; Remember N
   pushi #0        ; Push label
   pushstr "fac"   ; Push module name
   mcall
   loadrr r1 r0
   sys display     ; Print result
   addrri r1 r3 #2 ;
   pushi #0        ; Push label
   pushstr "tfac"  ; Push module name
   mcall
   loadrr r1 r0
   sys display     ; Print result
   ret
