LABEL 0          ; start/0
  ; Call summer(13, 10)
  PUSH 13        ; Pushes parameter M
  PUSH 10        ; Pushes parameter N
  CALL 1 2
  RET

LABEL 1          ; summer/2
  ; Is it summer(_M, 0) clause?
  PUSH -1
  LOAD FP       ; Parameter  N
  PUSH 0
  NEQ
  CJUMP 2
  PUSH 0
  RET

LABEL 2
  ; It is summer(M, N) clause!
  ; Call summer(M, N - 1)
  PUSH 0
  LOAD FP       ; Parameter M
  PUSH 0
  LOAD FP       ; Pushes Parameter M
  PUSH -1
  LOAD FP       ; Parameter N
  PUSH 1
  SUB            ; Pushes parameter N - 1
  CALL 1 2
  ADD            ; M + summer(M, N - 1)
  RET
