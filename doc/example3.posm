label 0          ; start/0
  push -1        ; Bogus return address
  push -1        ; Bogus fp
  push 1         ; Local variable X
  pushs "foo"    ; Local variable Y
  push -2
  load fp
  push 1
  add            ; Local variable Z
  ; Call bar(Y)
  push -3
  load fp        ; Parameter Y
  call 1 1
  ret copy

label 1          ; bar/2
  ; Call sys println
  push 0
  load fp        ; Parameter Y
  sys println
  pop            ; Ignores return value
  push 0
  load fp
  ret copy
