LABEL 0          ; start/0
  PUSH -1        ; Bogus return address
  PUSH -1        ; Bogus FP
  PUSH 1         ; Local variable X
  PUSHS "foo"    ; Local variable Y
  PUSH -2
  LOAD FP
  PUSH 1
  ADD            ; Local variable Z
  ; Call bar(Y)
  PUSH -3
  LOAD FP        ; Parameter Y
  CALL 1 1
  RET copy

LABEL 1          ; bar/2
  ; Call SYS println
  PUSH 0
  LOAD FP        ; Parameter Y
  SYS println
  POP            ; Ignores return value
  PUSH 0
  LOAD FP
  RET copy
